{
  "name": "edhrec-compare",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc -b && vite build",
    "build:analyze": "vue-tsc -b && cross-env ANALYZE=true vite build",
    "bundle:analyze": "cross-env ANALYZE=true vite build",
    "preview": "vite preview",
    "test:e2e": "playwright test",
    "test:unit": "vitest",
    "test:unit:watch": "vitest --watch",
    "test:unit:coverage": "vitest run --coverage",
    "test:unit:coverage:watch": "vitest --watch --coverage",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx,vue}\" --max-warnings=0",
    "lint:fix": "eslint \"src/**/*.{ts,tsx,js,jsx,vue}\" --fix",
    "format": "prettier --write .",
    "docs": "typedoc",
    "docs:serve": "typedoc && npx http-server docs/api -o",
    "issues:export": "node scripts/export-issues-for-boards.mjs",
    "size": "size-limit",
    "prepare": "husky"
  },
  "dependencies": {
    "@mdi/js": "^7.4.47",
    "@sentry/vue": "^10.30.0",
    "@tanstack/vue-virtual": "^3.13.13",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "@vueuse/core": "^14.1.0",
    "jsdom": "^27.3.0",
    "vue": "^3.5.25",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@playwright/test": "^1.57.0",
    "@size-limit/preset-app": "^12.0.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.47.0",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitest/coverage-v8": "^4.0.15",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.22",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.6.2",
    "globals": "^16.5.0",
    "happy-dom": "^20.0.11",
    "husky": "^9.1.6",
    "lint-staged": "^16.2.7",
    "postcss": "^8.4.49",
    "prettier": "^3.7.4",
    "rollup-plugin-visualizer": "^6.0.5",
    "size-limit": "^12.0.0",
    "tailwindcss": "^4.1.17",
    "typedoc": "^0.28.15",
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@7.2.2",
    "vitest": "^4.0.12",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.1.7"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.2"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,vue}": [
      "eslint --fix"
    ],
    "*.{cjs,mjs,json,md,css,scss,html,yml,yaml}": [
      "prettier --write"
    ]
  },
  "size-limit": [
    {
      "path": "dist/assets/index-*.js",
      "limit": "200 KB",
      "gzip": true
    },
    {
      "path": "dist/assets/index-*.css",
      "limit": "50 KB",
      "gzip": true
    }
  ]
}
