{
  "name": "edhrec-compare",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc -b && vite build",
    "build:analyze": "vue-tsc -b && cross-env ANALYZE=true vite build",
    "bundle:analyze": "cross-env ANALYZE=true vite build",
    "preview": "vite preview",
    "test:e2e": "playwright test",
    "test:unit": "vitest",
    "test:unit:watch": "vitest --watch",
    "test:unit:coverage": "vitest run --coverage",
    "test:unit:coverage:watch": "vitest --watch --coverage",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx,vue}\" --max-warnings=0",
    "lint:fix": "eslint \"src/**/*.{ts,tsx,js,jsx,vue}\" --fix",
    "format": "prettier --write .",
    "docs": "typedoc",
    "docs:serve": "typedoc && npx http-server docs/api -o",
    "issues:export": "node scripts/export-issues-for-boards.mjs",
    "size": "size-limit",
    "prepare": "husky"
  },
  "dependencies": {
    "@mdi/js": "^7.4.47",
    "@sentry/vue": "^10.36.0",
    "@tanstack/vue-virtual": "^3.13.18",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "@vueuse/core": "^14.1.0",
    "jsdom": "^27.4.0",
    "vue": "^3.5.27",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@playwright/test": "^1.58.2",
    "@size-limit/preset-app": "^12.0.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^25.2.2",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.47.0",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vitest/coverage-v8": "^4.0.18",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.24",
    "baseline-browser-mapping": "^2.9.19",
    "cross-env": "^10.1.0",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.7.0",
    "globals": "^17.3.0",
    "happy-dom": "^20.5.3",
    "husky": "^9.1.6",
    "lint-staged": "^16.2.7",
    "postcss": "^8.4.49",
    "prettier": "^3.8.1",
    "rollup-plugin-visualizer": "^6.0.5",
    "size-limit": "^12.0.0",
    "tailwindcss": "^4.1.17",
    "typedoc": "^0.28.16",
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@7.2.2",
    "vitest": "^4.0.12",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.2.4"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.2"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,vue}": [
      "eslint --fix"
    ],
    "*.{cjs,mjs,json,md,css,scss,html,yml,yaml}": [
      "prettier --write"
    ]
  },
  "size-limit": [
    {
      "path": "dist/assets/index-*.js",
      "limit": "200 KB",
      "gzip": true
    },
    {
      "path": "dist/assets/index-*.css",
      "limit": "50 KB",
      "gzip": true
    }
  ]
}
